{
  "name": "cloudflare-pages-test",
  "version": "0.0.1",
  "description": "",
  "main": "dist/_worker.js",
  "scripts": {
    "start": "npm run build && wrangler pages dev ./dist",
    "build": "npm run build-worker && npm run build-website",
    "build-website": "node ./scripts/buildSite.js",
    "build-worker": "node ./scripts/buildWorker.js",
    "ts": "npm run ts-worker && npm run ts-website",
    "ts-website": "tsc --noEmit --project ./website/tsconfig.json",
    "ts-worker": "tsc --noEmit --project ./worker/tsconfig.json",
    "format": "prettier --write  '*.{json,js}' './**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 . && prettier --check '*.{json,js}' './**/*.{js,ts}' 'test/**/*.{js,ts}'"
  },
  "author": "Vlady Veselinov",
  "license": "MIT OR Apache-2.0",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "dependencies": {
    "@benzene/http": "^0.4.0",
    "@graphql-tools/merge": "^8.2.1",
    "@graphql-tools/schema": "^8.3.1",
    "@prisma/client": "^3.8.1",
    "graphql": "16.2.0",
    "graphql-tag": "^2.12.6",
    "normalize.css": "^8.0.1",
    "prisma-proxy-fetch-client": "^0.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "worktop": "^0.8.0-next.10",
    "wrangler": "0.0.0-6e7baf2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.3.1",
    "@craftamap/esbuild-plugin-html": "^0.2.2",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/schema-ast": "2.4.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-resolvers": "2.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "dotenv": "^14.3.2",
    "esbuild": "^0.14.16",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "express": "^4.17.2",
    "jest": "^27.4.7",
    "miniflare": "^2.2.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.0",
    "prisma": "^3.8.1",
    "prisma-proxy-express-server": "^0.0.5",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^27.1.3",
    "typescript": "^4.3.2"
  }
}
