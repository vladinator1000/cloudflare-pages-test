{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "../dist",
    "module": "commonjs",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types"],
    "types": ["@cloudflare/workers-types", "@types/jest"]
  },
  "include": ["./"],
  "exclude": ["node_modules", "dist"]
}
